{
  "name": "desolid-admin",
  "version": "1.0.0",
  "description": "Desolid: admin panel",
  "author": "Sajjad Shirazy",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "format": "prettier --write \"**/*.js\" \"**/*.ts\" \"**/*.vue\" \"**/*.scss\"",
    "release:patch": "standard-version --release-as patch && git push && git push --tags",
    "release:minor": "standard-version --release-as minor && git push && git push --tags",
